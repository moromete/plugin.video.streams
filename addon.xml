<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.streams"
       name="Streams"
       version="2.0.5"
       provider-name="Moromete">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <!-- <import addon="script.module.pytz"/> -->
  </requires>
  <extension point="xbmc.python.pluginsource"
             library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>
      Radio/Tv Streams
    </summary>
    <summary lang="hr">
      Radio/Tv strujanja
    </summary>
    <description>
      Radio/Tv Streams
      - Watch internet video/audio streams (sopcast, acestream, http, rtmp, rtsp, mms ...) based on a remote channel list.
    </description>
    <description lang="hr">
      Radio/Tv strujanja
      - Gledajte internet video/audio strujanja (sopcast, acestream, http, rtmp, rtsp, mms ...) temeljena na udaljenom popisu programa.
    </description>
    <platform>all</platform>
  </extension>
</addon>
